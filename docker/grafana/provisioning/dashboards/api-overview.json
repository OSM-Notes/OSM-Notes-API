{
  "id": null,
  "uid": "osm-notes-api-overview",
  "title": "OSM Notes API - Overview",
  "tags": ["osm-notes", "api"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Requests per Second",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(http_requests_total[1m])",
          "legendFormat": "{{method}} {{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {}
        }
      }
    },
    {
      "id": 2,
      "title": "Request Rate by Status Code",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "rate(http_requests_total[1m])",
          "legendFormat": "{{status_code}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {}
        }
      }
    },
    {
      "id": 3,
      "title": "Response Time - P50, P95, P99",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P50",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "B",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "P99",
          "refId": "C",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {}
        }
      }
    },
    {
      "id": 4,
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "rate(http_errors_total[1m])",
          "legendFormat": "{{status_code}} - {{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {}
        }
      }
    },
    {
      "id": 5,
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "sum(increase(http_requests_total[1h]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "Error Count (Last Hour)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 16 },
      "targets": [
        {
          "expr": "sum(increase(http_errors_total[1h]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 100, "color": "red" }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 7,
      "title": "Average Response Time",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.5, "color": "yellow" },
              { "value": 1.0, "color": "red" }
            ]
          },
          "unit": "s"
        }
      }
    },
    {
      "id": 8,
      "title": "P95 Response Time",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 20 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1.0, "color": "yellow" },
              { "value": 2.0, "color": "red" }
            ]
          },
          "unit": "s"
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  }
}
