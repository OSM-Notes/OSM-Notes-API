{
  "id": null,
  "uid": "osm-notes-rate-limiting",
  "title": "OSM Notes API - Rate Limiting",
  "tags": ["osm-notes", "api", "rate-limiting"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Rate Limit Exceeded (per second)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "rate(rate_limit_exceeded_total[1m])",
          "legendFormat": "{{ip}} - {{user_agent}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {}
        }
      }
    },
    {
      "id": 2,
      "title": "Total Rate Limits Exceeded",
      "type": "stat",
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(increase(rate_limit_exceeded_total[1h]))",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 10, "color": "yellow"},
              {"value": 100, "color": "red"}
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "title": "Rate Limit Exceeded by IP",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 8, "y": 8},
      "targets": [
        {
          "expr": "topk(10, sum by (ip) (rate(rate_limit_exceeded_total[5m])))",
          "format": "table",
          "instant": true,
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "Rate Limit Exceeded by User-Agent",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "expr": "topk(10, sum by (user_agent) (rate(rate_limit_exceeded_total[5m])))",
          "format": "table",
          "instant": true,
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  }
}
