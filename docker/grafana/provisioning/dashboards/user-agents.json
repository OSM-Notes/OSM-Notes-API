{
  "id": null,
  "uid": "osm-notes-user-agents",
  "title": "OSM Notes API - User-Agents",
  "tags": ["osm-notes", "api", "user-agents"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Requests by Status Code",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum by (status_code) (rate(http_requests_total[5m]))",
          "legendFormat": "{{status_code}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "options": {}
    },
    {
      "id": 2,
      "title": "Top 10 Routes by Request Rate",
      "type": "bargauge",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "topk(10, sum by (route) (rate(http_requests_total[5m])))",
          "legendFormat": "{{route}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient"
      }
    },
    {
      "id": 3,
      "title": "Request Rate by Method",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum by (method) (rate(http_requests_total[1m]))",
          "legendFormat": "{{method}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "name": "Prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {}
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  }
}
