# Server Configuration
NODE_ENV=development
PORT=3000
API_VERSION=v1

# Database Configuration
# Connection to Analytics database (osm_notes_dwh)
DB_HOST=192.168.0.7
DB_PORT=5432
DB_NAME=osm_notes_dwh
DB_USER=analytics_user
DB_PASSWORD=your_password_here
DB_SSL=false

# Redis Configuration
REDIS_HOST=192.168.0.7
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=50
RATE_LIMIT_BOT_MAX_REQUESTS=10
RATE_LIMIT_BOT_WINDOW_MS=3600000

# OAuth Configuration (Fase 5)
OAUTH_ENABLED=false
OSM_OAUTH_CLIENT_ID=
OSM_OAUTH_CLIENT_SECRET=
OAUTH_CALLBACK_URL=http://localhost:3000/auth/callback

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Prometheus Metrics
METRICS_ENABLED=true
METRICS_PORT=9090

# API Documentation
API_DOCS_ENABLED=true
API_DOCS_PATH=/docs

# Security
TRUST_PROXY=false
CORS_ORIGIN=*
